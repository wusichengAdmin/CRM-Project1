<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.huashui.crm.workbench.dao.ClueRemarkDao">

    <delete id="deleteRemarkByClueId">

        delete from tbl_clue_remark where clueId=#{clueId}

    </delete>


    <select id="getRemarkListByClueId" resultType="ClueRemark">

        select * from tbl_clue_remark where clueId=#{clueId}

    </select>
  	
</mapper>

































